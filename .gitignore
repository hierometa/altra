# Ignore node_modules for both frontend and backend
node_modules/
frontend/node_modules/
backend/node_modules/

# Ignore build directories (you don't need to push dist or build folders)
dist/
frontend/dist/
backend/dist/
build/
frontend/build/
backend/build/

# Ignore lock files (optional, depending on how you manage your package-lock.json or yarn.lock)
package-lock.json
yarn.lock

# Ignore environment variables (never push sensitive information)
# .env
# .env.local
# frontend/.env
# backend/.env

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
lerna-debug.log*

# OS-specific files
.DS_Store

# IDE settings (VS Code specific settings)
.vscode/
.idea/

# Ignore Docker build artifacts
docker-compose.override.yml
docker-compose.prod.yml
frontend/.dockerignore
backend/.dockerignore

# Ignore temporary files
/tmp/
*.tmp

# Ignore coverage and test output (optional, if you use tests)
coverage/
test/

# Prisma-related build files
prisma/migrations/

# Docker images or volumes (optional)
docker-data/